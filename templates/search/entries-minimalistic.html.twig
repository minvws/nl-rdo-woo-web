
{# Results #}
{% if result.entries | length > 0 %}
<h2 class="visually-hidden">{{ "Results" | trans() }}</h2>
<ul class=" list-unstyled list-results">
    {% for entry in result.entries %}
    <li>
        {% include "search/entries/" ~ entry.type ~ ".html.twig" %}
    </li>
    {% endfor %}
</ul>
{% endif %}
