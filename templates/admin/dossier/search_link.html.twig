{% if dossiers | default([]) | length > 0 %}
    <ul>
        {% for dossier in dossiers %}
        <li class="py-2 px-3 border-b border-bhr-platinum" data-dossier-id="{{ dossier.id }}" role="option" aria-selected="false">
            {{ dossier.title }}
        </li>
        {% endfor %}
    </ul>
{% endif %}
