        "webmozart/assert": "^1.11",
        "xenolope/quahog": "^3.0"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "php-http/discovery": true,
            "phpstan/extension-installer": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "sort-packages": true,
        "audit": {
            "ignore": {
                "CVE-2024-36611": "See https://github.com/symfony/symfony/issues/59077"
            }
        }
    },
    "autoload": {
        "psr-4": {
            "Shared\\": "src/",
            "Admin\\": "apps/admin/src/",
            "Public\\": "apps/public/src/",
            "PublicationApi\\": "apps/publication_api/src/",
            "Worker\\": "apps/worker/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Shared\\Tests\\": "tests/",
            "Admin\\Tests\\": "apps/admin/tests/",
            "Public\\Tests\\": "apps/public/tests/",
            "PublicationApi\\Tests\\": "apps/publication_api/tests/",
            "Worker\\Tests\\": "apps/worker/tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*"
    },
    "scripts": {
        "pre-autoload-dump": "Aws\\Script\\Composer\\Composer::removeUnusedServices",
        "auto-scripts": {
            "cache:clear -ishared": "symfony-cmd",
            "cache:clear -iadmin": "symfony-cmd",
            "cache:clear -ipublic": "symfony-cmd",
            "cache:clear -ipublication_api": "symfony-cmd",
            "cache:clear -iworker": "symfony-cmd",
            "assets:install %PUBLIC_DIR% -iadmin": "symfony-cmd",
            "assets:install %PUBLIC_DIR% -ipublic": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts",
            "@openapi:export"
        ],
        "post-update-cmd": [
            "@auto-scripts",
            "@openapi:export"
        ],
        "syncdotenv": "phpdotenvsync --opt=sync --src=.env.development --dest=.env.local --no-interaction",
        "checkstyle": [
            "XDEBUG_MODE=off phpcs",
            "XDEBUG_MODE=off php-cs-fixer fix --dry-run --diff --verbose"
        ],
        "fixstyle": [
            "XDEBUG_MODE=off phpcbf",
            "XDEBUG_MODE=off php-cs-fixer fix --diff"
        ],
        "checktype": [
            "XDEBUG_MODE=off bin/console cache:warmup --no-optional-warmers",
            "XDEBUG_MODE=off phpstan analyse --memory-limit=-1"
        ],
        "update-type-baseline": [
            "XDEBUG_MODE=off bin/console cache:warmup --no-optional-warmers",
            "XDEBUG_MODE=off phpstan --generate-baseline"
        ],
        "checktranslations": "XDEBUG_MODE=off bin/console lint:yaml translations",
        "checktemplates": [
            "XDEBUG_MODE=off APP_DEBUG=false php bin/console cache:clear",
            "XDEBUG_MODE=off APP_DEBUG=false php bin/console lint:twig templates"
        ],
        "checkall": [
            "@checkstyle",
            "@checktype",
            "@checkarc",
            "@checktranslations",
            "@checktemplates",
            "@test",
            "composer audit",
            "@rector-preview"
        ],
        "test": [
            "Composer\\Config::disableProcessTimeout",
            "XDEBUG_MODE=off phpunit -d --without-creating-snapshots"
        ],
        "openapi:export": [
            "XDEBUG_MODE=off bin/console -ipublication_api api:openapi:export --output public/api/publication/v1/openapi.json",
            "XDEBUG_MODE=off bin/console -ipublication_api api:openapi:export --output public/api/publication/v1/openapi.yaml --yaml",
            "XDEBUG_MODE=off bin/console -iadmin api:openapi:export --output public/api/admin/openapi.json",
            "XDEBUG_MODE=off bin/console -iadmin api:openapi:export --output public/api/admin/openapi.yaml --yaml"
        ],
        "test-report": [
            "Composer\\Config::disableProcessTimeout",
            "XDEBUG_MODE=off php -dpcov.enabled=1 -dpcov.directory=. vendor/bin/phpunit -d --without-creating-snapshots --coverage-text --coverage-html coverage/html"
        ],
        "update-test-snapshots": "XDEBUG_MODE=off phpunit -d --update-snapshots",
        "rector": "rector",
        "rector-preview": "rector --dry-run",
        "checkarc": "phparkitect check"
    },
    "scripts-descriptions": {
        "syncdotenv": "Synchronize .env.development with .env.local",
        "checkstyle": "Check code style using php_codesniffer and php-cs-fixer",
        "fixstyle": "Fix code style using php_codesniffer and php-cs-fixer",
        "checktype": "Check code type using PHPStan",
        "update-type-baseline": "Update PHPStan baseline",
        "checktranslations": "Run YAML linting on translation files",
        "checktemplates": "Run Twig linting on templates",
        "checkall": "Run all checks including test",
        "test": "Run PHPUnit tests",
        "test-report": "Run PHPUnit tests with coverage report",
        "update-test-snapshots": "Update PHPUnit snapshots",
        "rector": "Run Rector on the code base",
        "rector-preview": "Preview Rector changes on the code base",
        "checkarc": "Check architecture rules using PHPArkitect"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "6.4.*"
        },
        "aws/aws-sdk-php": [
            "S3"
        ]
    },
    "require-dev": {
        "dama/doctrine-test-bundle": "^8.2",
        "dg/bypass-finals": "^1.5",
        "friendsofphp/php-cs-fixer": "^3.91.2",
        "league/flysystem-memory": "^3.0",
        "mikey179/vfsstream": "^1.6",
        "mockery/mockery": "^1.6.12",
        "phparkitect/phparkitect": "^0.7.0",
        "phpcompatibility/php-compatibility": "^10.0.0-alpha1",
        "phpstan/extension-installer": "^1.4",
        "phpstan/phpstan": "^2.0",
        "phpstan/phpstan-doctrine": "^2.0",
        "phpstan/phpstan-mockery": "^2.0",
        "phpstan/phpstan-symfony": "^2.0",
        "phpstan/phpstan-webmozart-assert": "^2.0",
        "phpunit/phpunit": "^12.4",
        "rector/rector": "^2.0",
        "slevomat/coding-standard": "^8.25",
        "spatie/phpunit-snapshot-assertions": "^5.1",
        "squizlabs/php_codesniffer": "^4.0",
        "stefanocbt/phpdotenv-sync": "^1.2",
        "swisnl/phpstan-faker": "^1.0",
        "symfony/browser-kit": "6.4.*",
        "symfony/css-selector": "6.4.*",
        "symfony/debug-bundle": "6.4.*",
        "symfony/maker-bundle": "^1.0",
        "symfony/web-profiler-bundle": "6.4.*",
        "zenstruck/foundry": "^2.0"
    }
}
